{
  "name": "artibot",
  "version": "1.4.0",
  "description": "Artibot - Claude Code orchestration plugin runtime",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "validate": "node scripts/validate.js",
    "lint": "eslint lib/ scripts/ tests/",
    "lint:fix": "eslint --fix lib/ scripts/ tests/",
    "ci": "node scripts/validate.js && npm run lint && npm test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "vitest": "^3.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0"
  }
}
