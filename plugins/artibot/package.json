{
  "name": "artibot",
  "version": "1.4.0",
  "description": "Artibot - Claude Code orchestration plugin runtime",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "validate": "node scripts/validate.js",
    "lint": "eslint lib/ scripts/ tests/",
    "lint:fix": "eslint --fix lib/ scripts/ tests/",
    "ci": "node scripts/validate.js && npm run lint && npm test",
    "ci:coverage": "node scripts/validate.js && npm run lint && node scripts/ci/validate-coverage.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "node scripts/ci/validate-coverage.js --check"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "vitest": "^4.0.18"
  }
}
